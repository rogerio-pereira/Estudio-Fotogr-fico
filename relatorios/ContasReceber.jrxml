	<queryString>
		<![CDATA[SELECT
     empresa.`cnpj` AS empresa_cnpj,
     empresa.`Endereco` AS empresa_Endereco,
     empresa.`bairro` AS empresa_bairro,
     empresa.`cep` AS empresa_cep,
     empresa.`cidade` AS empresa_cidade,
     empresa.`complemento` AS empresa_complemento,
     empresa.`email` AS empresa_email,
     empresa.`estado` AS empresa_estado,
     empresa.`fax` AS empresa_fax,
     empresa.`nome` AS empresa_nome,
     empresa.`numero` AS empresa_numero,
     empresa.`site` AS empresa_site,
     empresa.`telefone` AS empresa_telefone,
     vendas.`codigo` AS vendas_codigo,
     vendas.`dia` AS vendas_dia,
     clientes.`nome` AS clientes_nome,
     tiposeventos.`tipo` AS tiposeventos_tipo
FROM
     `clientes` clientes INNER JOIN `vendas` vendas ON clientes.`codigo` = vendas.`cliente_codigo`
     INNER JOIN `tiposeventos` tiposeventos ON vendas.`tipoEvento_codigo` = tiposeventos.`codigo`,
     `empresa` empresa
WHERE
      clientes.`nome` LIKE '%$P!{Cliente}%'
	AND tiposeventos.`tipo` LIKE '%$P!{Evento}%'
	and (vendas.quitado = $P{Quitado1} or vendas.quitado = $P{Quitado2})
ORDER BY
     vendas_codigo ASC]]>
	</queryString>
